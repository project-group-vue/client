        </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</section>\n</div>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n    data() {\n        return {\n            email: '',\n            password: ''\n        }\n    },\n    methods: {\n        login() {\n          Swal.showLoading()\n            axios({\n                url: \"http://localhost:3000/user/login\",\n                method: \"POST\",\n                data:{\n                  email: this.email,\n                  password: this.password\n                }\n            })\n            .then(({data})=>{\n              Swal.close()\n              Swal.fire({\n                icon: 'success',\n                title: 'Success',\n                text: 'User logged in!',\n              })\n                console.log(data)\n                this.email = \"\"\n                this.password = \"\"\n            })\n            .catch(err => {\n              Swal.close()\n              Swal.fire({\n                icon: 'error',\n                title: 'Oops...',\n                text: `${err.response.data.message}`,\n              })\n                console.log(err.response.data)\n            })\n        },\n        registerForm(){\n            return this.$emit('registerForm')\n        }\n    }\n}\n</script>\n\n<style>\n.fdb-block{\n    height: 100vh;\n    background-image: url('../../img/bg-masthead.jpg')\n}\n</style>"},"lineCount":null}},"error":null,"hash":"a5868a9e03ea6f7fa7436feec5c96716","cacheData":{"env":{}}}